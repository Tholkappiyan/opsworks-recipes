production:
  adapter: <%= @database[:adapter].to_s.inspect %>
  database: <%= @database[:database_name].to_s.inspect %>
  encoding: utf8
  host: <%= @database[:host].to_s.inspect %>
  port: <%= @database[:port].to_s.inspect %>
  username: <%= @database[:user_name].to_s.inspect %>
  password: <%= @database[:password].to_s.inspect %>
  reconnect: <%= @database[:reconnect].to_s.inspect %>
